{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS Debug",
        "runtimeArgs": ["--inspect"],
        "args": ["dist/main.js"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal"
      }
    ]
  }
  